{
    "compilerOptions": {
      "baseUrl": "src",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "target": "es2019",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "types": [
        "@emotion/react"
      ],
      "plugins": [],
      "noFallthroughCasesInSwitch": true
    },
    "include": [
      "src"
    ]
  }
  